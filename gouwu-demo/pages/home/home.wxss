swiper {
  width: 100%;
  height: 340rpx;
}
swiper image {
  width: 100%;
  height: 340rpx;
}
.menus {
  display: flex;
  justify-content: space-around;
  padding-top: 24rpx;
  padding-bottom: 29rpx;
}
.menus .menus-item image {
  width: 128rpx;
  height: 128rpx;
}
.floor .floor-title {
  background-color: #f4f4f4;
  padding-top: 30rpx;
}
.floor .floor-title image {
  width: 100%;
  height: 59rpx;
}
.floor .floor-body {
  display: flex;
  padding: 20rpx 16rpx;
}
.floor .floor-body .left {
  height: 386rpx;
  padding-right: 10rpx;
}
.floor .floor-body .left image {
  width: 232rpx;
  height: 386rpx;
}
.floor .floor-body .right {
  height: 386rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.floor .floor-body .right image {
  width: 233rpx;
  height: 188rpx;
}
.top {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #fff;
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
[hidden] {
  display: none !important;
}
